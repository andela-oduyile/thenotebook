<%= form_for @note do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :content %><br /><br />
    <%= f.text_area :content, :class => "tinymce", :rows => 400, :cols => 120 %>
    <%= tinymce %>
  </div><br />
  <div class="actions row">
    <div class="col s2 offset-s10">
      <%= f.submit :class => "col s12 btn waves-effect waves-light amber" %>
    </div>
  </div>
<% end %>
